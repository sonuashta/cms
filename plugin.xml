<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.intut.luckylottery.cms.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="com.intut.luckylottery.cms.Perspective"
            id="com.intut.luckylottery.cms.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="Lucky Lottery"
            class="com.intut.luckylottery.cms.View"
            id="com.intut.luckylottery.cms.view">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               standalone="true"
               minimized="false"
               relative="org.eclipse.ui.editorss"
               relationship="left"
               id="com.intut.luckylottery.cms.view">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="com.intut.luckylottery.cms.toolbar2">
            <command
                  commandId="com.intut.luckylottery.cms.singleCustomerCommand"
                  label="Single Lottery Command"
                  mode="FORCE_TEXT"
                  style="push">
            </command>
            <command
                  commandId="com.intut.luckylottery.cms.multiCustomerCommand"
                  label="MulticustomerCommand"
                  mode="FORCE_TEXT"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.intut.luckylottery.cms.commands.SingleCustomerCommand"
            id="com.intut.luckylottery.cms.singleCustomerCommand"
            name="Single Customer Command">
      </command>
      <command
            defaultHandler="com.intut.luckylottery.cms.commands.MultiCustomerCommand"
            id="com.intut.luckylottery.cms.multiCustomerCommand"
            name="Multi Customer Command">
      </command>
   </extension>

</plugin>
